@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-collection_fill:before { content: "\e6ea"; }

.icon-emoji_fill:before { content: "\e6f6"; }





/**me.wxss**/
.me-bg {
  width: 100%;
  height: 400rpx;
}
.me {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.me-avatar { 
  position: absolute;
  margin-top: 100rpx;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.me-nickname {
  position: absolute;
  margin-top: 250rpx;
  color: #fff;
}
.me-center {
  position: absolute;
  margin-top: 300rpx;
  color: #fff;
}
.about-list {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 10rpx;
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #e2e2e2;
  color: #333;
}
